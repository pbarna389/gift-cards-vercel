{
	"name": "giftcard-project",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"devServer": "json-server --watch db.json",
		"build": "tsc && vite build",
		"lint": "tsc --noEmit && eslint src/**/*.ts{,x} --cache --max-warnings=0 && stylelint './src/**/*.ts{,x}'",
		"lint:fix": "tsc --noEmit && eslint src/**/*.ts{,x} --cache --max-warnings=0 --fix && stylelint './src/**/*.ts{,x}' --fix",
		"lint:code": "tsc --noEmit && eslint src/**/*.ts{,x} --cache --max-warnings=0",
		"lint:code-fix": "tsc --noEmit && eslint src/**/*.ts{,x} --fix",
		"lint:css": "stylelint './src/**/*.ts{,x}' && ",
		"lint:css-fix": "stylelint './src/**/*.ts{,x}' --fix",
		"test": "vitest",
		"test:run": "vitest run",
		"coverage": "vitest run --coverage",
		"preview": "vite preview",
		"prepare": "husky install"
	},
	"dependencies": {
		"@react-oauth/google": "^0.11.1",
		"@types/node": "^20.6.5",
		"eslint-plugin-no-relative-import-paths": "^1.5.2",
		"eslint-plugin-require-explicit-generics": "^0.4.2",
		"formik": "^2.4.3",
		"jsdom": "^22.1.0",
		"json-server": "^0.17.3",
		"jwt-decode": "^3.1.2",
		"pure-react-carousel": "^1.30.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-icons": "^4.10.1",
		"react-lazy-load": "^4.0.1",
		"react-lazy-load-image-component": "^1.6.0",
		"react-particles": "^2.12.2",
		"react-router-dom": "^6.15.0",
		"styled-components": "^6.0.7",
		"swiper": "^10.2.0",
		"tsparticles-slim": "^2.12.0",
		"vite-tsconfig-paths": "^4.2.1",
		"yup": "^1.2.0"
	},
	"devDependencies": {
		"@rollup/plugin-replace": "^5.0.2",
		"@testing-library/jest-dom": "^5.17.0",
		"@testing-library/react": "^14.0.0",
		"@testing-library/user-event": "^14.4.3",
		"@types/react": "^18.2.15",
		"@types/react-dom": "^18.2.7",
		"@types/react-lazy-load-image-component": "^1.6.1",
		"@typescript-eslint/eslint-plugin": "^5.62.0",
		"@typescript-eslint/parser": "^5.62.0",
		"@vitejs/plugin-react": "^4.0.3",
		"@vitest/coverage-v8": "^0.34.1",
		"@vitest/ui": "^0.34.1",
		"eslint": "^8.48.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-airbnb-typescript": "^17.1.0",
		"eslint-config-prettier": "^8.10.0",
		"eslint-import-resolver-typescript": "^3.6.0",
		"eslint-plugin-import": "^2.28.0",
		"eslint-plugin-jsx-a11y": "^6.7.1",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react": "^7.33.1",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.3",
		"eslint-plugin-simple-import-sort": "^10.0.0",
		"husky": "^8.0.3",
		"msw": "^1.2.3",
		"postcss-styled-syntax": "^0.4.0",
		"prettier": "^2.8.8",
		"stylelint": "^15.10.2",
		"stylelint-config-standard": "^34.0.0",
		"typescript": "^5.1.6",
		"vite": "^4.4.5",
		"vitest": "^0.34.1"
	}
}
